  /* SIDE ONE */
  .values-card .side-one {
    -webkit-transition: all 0.4s ease-in-out;  -moz-transition: all 0.4s ease-in-out; -o-transition: all 0.4s ease-in-out;
    transition: all 0.4s ease-in-out;
    margin-bottom: 20px;
  }

  /* SIDE TWO */
  .values-card .side-two {
    opacity: 0;
    -webkit-transform: translateY(30px);
    -ms-transform: translateY(30px);
    -o-transform: translateY(30px);
    transform: translateY(30px);
    -webkit-transition: all 0.4s ease-in-out;
    -moz-transition: all 0.4s ease-in-out;
    -o-transition: all 0.4s ease-in-out;
    transition: all 0.4s ease-in-out;
    height: 200px;
    position: absolute;
    top: 0;
    left: 0;
    padding: 20px;
    text-align: left;
  }

  .values-card .side-two p {
    text-align: left;
  }



  /* SIDE HOVER  */
  .values-card:hover .side-two {
    opacity: 1;
    -webkit-transform: translateY(0px);
    -ms-transform: translateY(0px);
    -o-transform: translateY(0px);
    transform: translateY(0px);
  }

  .values-card:hover .side-one {
    opacity: 0;
    -webkit-transform: translateY(-80px);
    -ms-transform: translateY(-80px);
    -o-transform: translateY(-80px);
    transform: translateY(-80px);

    -webkit-transition: all 0.1s ease-in-out; -moz-transition: all 0.1s ease-in-out; -o-transition: all 0.1s ease-in-out;
    transition: all 0.1s ease-in-out;
