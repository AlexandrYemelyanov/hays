.staff-profile{
  position: relative;
  margin: 0px 0px 55px;
}

.staff-profile:after {
  content: ".";
  visibility: hidden;
  display: block;
  height: 0;
  clear: both;
}

.staff-profile.alt > .staff-profile--img{
  float:right;
}

.staff-profile.alt >.staff-profile--quote {
  float: right;
  margin-right: 40px;
  margin-left: 0;
}

.staff-profile.alt > .staff-profile--quote:before {
  content: "";
  display: block;
  width: 0;
  height: 0;
  border-top: 15px solid transparent;
  border-bottom: 15px solid transparent;
  border-left: 15px solid #fff;
  border-right: none;
  position: absolute;
  top: 50px;
  right: 0;
  left: 100%;
}

.staff-profile--img{
  width: 300px;
  float: left;
}

.staff-profile--img img {
  width: 100%;
}

.staff-profile--text{
  padding: 15px;
  background: #002776;
}

.staff-profile--text h3{
  font-size: 1.2em!important;
  text-transform: uppercase;
  margin-bottom: 10px!important;
}
.staff-profile--text h4{
  font-size: 1em!important;
  margin-bottom: 0!important;
}

.staff-profile--text h3, .staff-profile--text h4{
  color: #fff!important;

}

.staff-profile--quote{
  background: #fff;
  float: left;
  padding: 20px;
  width: 69%;
  margin-left: 40px;
  position: relative;
}


.staff-profile--quote:before {
  content: "";
  display: block;
  width: 0;
  height: 0;
  border-top: 15px solid transparent;
  border-bottom: 15px solid transparent;
  border-right: 15px solid #fff;
  position: absolute;
  top: 50px;
  left: -12px;
}



